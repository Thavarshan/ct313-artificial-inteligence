   "source": [
    "numbers = [3, 4, 67, 41, 3, 27, 23, 90, 3, 4, 1, 39, 14]\n",
    "sum_of_nums = 0\n",
    "count = 0\n",
    "for number in numbers:\n",
    "    sum_of_nums += number\n",
    "    count += 1\n",
    "average = sum_of_nums / count\n",
    "print(\"The list of numbers is:\", numbers)\n",
    "print(\"The average of all the numbers is:\", round(average, 2))"
   ]
